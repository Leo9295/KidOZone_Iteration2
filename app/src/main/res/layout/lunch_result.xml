<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageFood1"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toLeftOf="@+id/imagelucnchBox"
        android:layout_marginTop="250dp"
        android:layout_marginRight="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageFood2"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toLeftOf="@+id/imagelucnchBox"
        android:layout_below="@+id/imageFood1"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageFood3"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toLeftOf="@+id/imagelucnchBox"
        android:layout_below="@+id/imageFood2"
        android:layout_marginTop="30dp"
        android:layout_marginRight="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageFood4"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toRightOf="@+id/imagelucnchBox"
        android:layout_marginTop="250dp"
        android:layout_marginLeft="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageFood5"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toRightOf="@+id/imagelucnchBox"
        android:layout_below="@+id/imageFood4"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageFood6"
        android:layout_width="58dp"
        android:layout_height="61dp"
        android:layout_toRightOf="@+id/imagelucnchBox"
        android:layout_below="@+id/imageFood5"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="10dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imagelucnchBox"
        android:layout_width="237dp"
        android:layout_height="202dp"
        android:layout_centerInParent="true"
        app:srcCompat="@drawable/lunchresult" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="match_parent"
        android:layout_height="42dp"
        android:layout_marginBottom="44dp"
        android:gravity="center"
        android:layout_marginTop="30dp"
        android:text="You get        score."
        android:layout_below="@+id/imageFood3"
        android:textSize="24sp" />



    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Back"
        android:background="@drawable/button_style1"/>
</RelativeLayout>